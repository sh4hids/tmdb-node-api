{
  "name": "tmdb-node-api",
  "version": "1.0.0",
  "description": "A koa starter kit",
  "main": "index.js",
  "engines": {
    "node": "14.17.1"
  },
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon dev.js",
    "build": "babel --delete-dir-on-start --out-dir build --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "lint": "eslint --ignore-path .gitignore --ext .js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm-run-all --parallel check-format lint test build",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "sync-data": "node ./data/index.js"
  },
  "author": "Shahidul Islam Majumder <hello@shahid.pro>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-pino-logger": "^3.0.0",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "minimist": "^1.2.5",
    "mysql2": "^2.3.0",
    "node-cron": "^3.0.0",
    "pino": "^6.13.2",
    "query-string": "^7.0.1",
    "sequelize": "^6.6.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@trivago/prettier-plugin-sort-imports": "^2.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.0.1",
    "prettier": "^2.4.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6"
  }
}
